# AgentOS Ecosystem - Environment Configuration Template
# Copy this file to .env and configure with your actual values

# ===================================
# CORE CONFIGURATION
# ===================================
GO_ENV=development
NODE_ENV=development
LOG_LEVEL=debug

# ===================================
# DATABASE CONFIGURATION
# ===================================
DATABASE_URL=postgres://agentos:agentos_dev_password@localhost:5432/agentos_dev?sslmode=disable
REDIS_URL=redis://localhost:6379/0
NATS_URL=nats://localhost:4222

# ===================================
# SERVICE PORTS
# ===================================
CORE_API_PORT=8000
AGENT_ENGINE_PORT=8001
MEMORY_SERVICE_PORT=8002
TOOL_REGISTRY_PORT=8003

# ===================================
# SECURITY
# ===================================
JWT_SECRET=dev-jwt-secret-change-in-production
API_RATE_LIMIT=10000

# ===================================
# AI SERVICES (OPTIONAL)
# ===================================
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
PINECONE_API_KEY=your-pinecone-api-key-here

# ===================================
# MONITORING
# ===================================
PROMETHEUS_ENABLED=true
JAEGER_ENABLED=true
